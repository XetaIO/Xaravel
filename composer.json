{
    "name": "xetaio/xetaravel",
    "description": "A light version of Xeta's website using Laravel.",
    "keywords": [
        "Xeta",
        "framework",
        "laravel",
        "website",
        "blog",
        "administration"
    ],
    "homepage": "https://github.com/XetaIO/Xetaravel",
    "license": "MIT",
    "type": "project",
    "support": {
        "source": "https://github.com/XetaIO/Xetaravel",
        "issues": "https://github.com/XetaIO/Xetaravel/issues"
    },
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "spatie/laravel-medialibrary": "^5.0.0",
        "spatie/laravel-menu": "^2.1",
        "spatie/laravel-analytics": "^3.0",
        "xetaio/xetaravel-local": "^0.0.1",
        "xetaio/xetaravel-iptraceable": "^0.0.1",
        "xetaio/xetaravel-editor-md": "^0.0.1",
        "xetaio/xetaravel-mentions": "~0.0.1",
        "xety/breadcrumbs": "~1.0",
        "fideloper/proxy": "~3.3",
        "laravelcollective/html": "^5.4.0",
        "greggilbert/recaptcha": "~2.0",
        "kirkbushell/eloquence": "~2.0",
        "ultraware/roles": "^5.4",
        "rinvex/country": "^3.1",
        "graham-campbell/markdown": "^7.0",
        "webuni/commonmark-table-extension": "^0.6.1",
        "laravel/socialite": "^3.0",
        "league/color-extractor": "^0.3.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/filesystem": "^3.2",
        "squizlabs/php_codesniffer": "^3.0",
        "phpro/grumphp": "^0.11.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Xetaravel\\": "app/",
            "XetaravelInstaller\\": "install/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "npm install",
            "npm run development",
            "XetaravelInstaller\\Installer::postInstall"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 2000
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
